<% if @user.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      The forms contains <%= pluralize(@user.errors.count,"erorr") %>.
    </div>
    <ul>
      <%= @user.errors.full_messages.each do |msg| %>
        <li> <%= msg %> </li>
      <% end %>
    </ul>
  </div>
<% end %>